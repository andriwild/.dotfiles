{
    "window.menuBarVisibility": "toggle",
    "editor.minimap.enabled": false,
    "zig.initialSetupDone": true,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "zig.path": "/opt/zig/zig",
    "zig.zls.path": "/home/andri/.config/Code/User/globalStorage/ziglang.vscode-zig/zls_install/zls",
    "zig.formattingProvider": "off",
    "zig.zls.enableInlayHints": false,

    "workbench.colorTheme": "Visual Studio Dark",

    "editor.lineNumbers": "relative",
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.leader": "<space>",
    "vim.useSystemClipboard": true,
    "vim.vimrc.enable": true,
    "vim.vimrc.path": "/home/andri/.config/vimrc",
    "vim.normalModeKeyBindingsNonRecursive": [
    {
        "before": ["<leader>", "t"],
        "commands": ["workbench.action.terminal.toggleTerminal"]
    },
    {
        "before": ["<leader>", "v", "d"],
        "commands": ["editor.action.showHover"]
    },
    {
        "before": ["<leader>", "enter"],
        "commands": ["editor.action.quickFix"]
    },
    {
        "before": ["K"],
        "commands": ["editor.action.showHover"]
    },
    {
        "before": ["<leader>", "s", "f"],
        "commands": ["workbench.action.quickOpen"]
    },
    {
        "before": ["<leader>", "g", "f"],
        "commands": ["workbench.action.findInFiles"]
    },
    {
        "before": [ "<leader>", "2"],
        "commands": [ "editor.action.marker.next"]
    }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
    {
        "before": ["<leader>", "y"],
        "commands": [ "editor.action.clipboardCopyAction" ]
    }
],
"jupyter.widgetScriptSources": [
    "jsdelivr.com",
    "unpkg.com"
],
"python.defaultInterpreterPath": "/home/andri/fhnw/MSE/MachLe/notebooks/.venv",
"github.copilot.chat.welcomeMessage": "never",
"github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false
},
"workbench.iconTheme": "file-icons",
"files.associations": {
    "*.xacro": "xml"
},
"redhat.telemetry.enabled": false,
"remote.autoForwardPortsSource": "hybrid"
}
